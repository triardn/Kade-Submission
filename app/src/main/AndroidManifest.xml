<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.triardn.kadesubmission">
    
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.triardn.kadesubmission.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.triardn.kadesubmission.DetailLeagueActivity"></activity>
        <activity android:name="com.triardn.kadesubmission.ClubListActivity"></activity>
        <activity android:name="com.triardn.kadesubmission.MatchScheduleActivity"></activity>
        <activity android:name="com.triardn.kadesubmission.DetailMatchActivity"></activity>
        <activity android:name="com.triardn.kadesubmission.SearchActivity"></activity>
        <activity android:name="com.triardn.kadesubmission.DetailTeamActivity"></activity>
        <activity android:name="com.triardn.kadesubmission.SearchTeamActivity"></activity>
        <activity android:name="com.triardn.kadesubmission.StandingsActivity"></activity>
    </application>

</manifest>